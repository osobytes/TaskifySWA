{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.{css,scss,js,png,gif,ico,jpg,svg}",
            "assets/*",
            ".auth/*"
        ]
    },
    "routes": [
        {
          "route": "/api/*",
          "allowedRoles": ["authenticated"]
        }
    ],
    "mimeTypes": {
        ".json": "text/json"
    }
}